<%- include('../partials/head') %>
    <h1>Edit <%=foundListing.title%></h1>
    <form action="/listings/<%= foundListing._id%>?_method=PUT" method="POST" enctype="multipart/form-data">
        <label for="title">Title</label>
        <input type="text" name="title" id="title" value="<%= foundListing.title%>" required>
        <label for="location">Location</label>
        <input type="text" name="location" id="location" value="<%= foundListing.location %>" required>
        <label for="description">Description</label>
        <textarea name="description" id="description" required><%= foundListing.description%></textarea>
        <label for="imageurl">Image url</label>
        <input type="file" name="imageurl" id="imageurl" value="<%= foundListing.imageurl%>" required>
        <label for="titleImageUrl">Title for image</label>
        <input type="text" name="titleImageUrl" id="titleImageUrl" value="<%= foundListing.titleImageUrl%>" required>
        <button type="submit">Complete</button>
        
    </form>
</body>
</html>