<%- include('../partials/head') %>
    <h1>Edit <%=foundListing.title%></h1>
    <form action="/listings/<%= foundListing._id%>?_method=PUT" method="POST">
        <label for="title">Title</label>
        <input type="text" name="title" id="title" value="<%= foundListing.title%>">
        <label for="location">Location</label>
        <input type="text" name="location" id="location" value="<%= foundListing.location %>">
        <label for="description">Description</label>
        <textarea name="description" id="description"><%= foundListing.description%></textarea>
        <label for="imageurl">Image url</label>
        <input type="text" name="imageurl" id="imageurl" value="<%= foundListing.imageurl%>">
        <label for="titleImageUrl">Title for image</label>
        <input type="text" name="titleImageUrl" id="titleImageUrl" value="<%= foundListing.titleImageUrl%>">
        <button type="submit">Complete</button>
        
    </form>
</body>
</html>